<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <Button
        android:id="@+id/btnOpenGpsTestGps"
        android:layout_marginTop="30dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="OpenGps" />

    <Button
        android:id="@+id/btnCloseGpsTestGps"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="CloseGps" />


    <TextView
        android:id="@+id/textLongitudeTestGps"
        android:layout_width="fill_parent"
        android:textSize="20sp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textLatitudeTestGps"
        android:layout_width="fill_parent"
        android:textSize="20sp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textProviderTestGps"
        android:layout_width="fill_parent"
        android:textSize="20sp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textStatusChangedTestGps"
        android:layout_width="fill_parent"
        android:textSize="20sp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textGpsStatusChangedTestGps"
        android:layout_width="fill_parent"
        android:textSize="20sp"
        android:layout_height="wrap_content" />
</LinearLayout>
